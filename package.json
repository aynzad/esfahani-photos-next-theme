{
  "name": "esfahani-photos-next-theme",
  "version": "0.0.1",
  "description": "The esfahani.photo is a multilingual NextJs theme which aims to modernize, organize and enhance some aspects of a personal portfolio theme for photographers",
  "author": "Alireza Esfahani <alireza@esfahani.dev>",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "concurrently \"npm:next:dev\" \"npm:slicemachine\" --names \"next,slicemachine\" --prefix-colors blue,magenta",
    "next:dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "lint": "next lint",
    "slicemachine": "start-slicemachine",
    "format": "prettier --write .",
    "gen": "prismic-ts-codegen",
    "prepare": "husky install"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "endOfLine": "auto"
  },
  "dependencies": {
    "@plaiceholder/next": "^2.5.0",
    "@prismicio/client": "^6.7.1",
    "@prismicio/helpers": "^2.3.5",
    "@prismicio/next": "^0.1.5",
    "@prismicio/react": "^2.5.0",
    "@prismicio/slice-simulator-react": "^0.2.2",
    "clsx": "^1.2.1",
    "imgix-url-builder": "^0.0.3",
    "keen-slider": "^6.8.2",
    "next": "^12.3.1",
    "nextjs-progressbar": "^0.0.14",
    "node-fetch": "^3.2.10",
    "plaiceholder": "^2.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "^17.4.0",
    "tailwind-nth-child": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/prompt-cli": "^8.0.0",
    "@prismicio/types": "^0.2.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "@types/node": "18.7.23",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "autoprefixer": "^10.4.11",
    "concurrently": "^7.3.0",
    "eslint": "^8.24.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prismic-ts-codegen": "^0.1.5",
    "slice-machine-ui": "^0.5.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aynzad/esfahani-photos-next-theme.git"
  },
  "bugs": {
    "url": "https://github.com/aynzad/esfahani-photos-next-theme/issues"
  },
  "homepage": "https://github.com/aynzad/esfahani-photos-next-theme#readme"
}
